source 'https://rubygems.org'

# GitHub Pages와 Jekyll의 호환성을 보장합니다.
gem "github-pages", group: :jekyll_plugins

# 필요에 따라 추가 플러그인들 설정
gem 'jekyll-archives'
gem 'jekyll-sitemap'
gem 'jekyll-paginate-v2'
gem "kramdown-parser-gfm", "~> 1.1"


