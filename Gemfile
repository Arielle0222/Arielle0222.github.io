source 'https://rubygems.org'

# GitHub Pages와 Jekyll의 호환성을 보장합니다.
gem "github-pages", group: :jekyll_plugins

# 페이지네이션 플러그인
gem 'jekyll-paginate'

# 추가 플러그인
gem 'jekyll-archives'
gem 'jekyll-sitemap'
gem "kramdown-parser-gfm", "~> 1.1"



